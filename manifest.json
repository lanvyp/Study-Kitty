{
  "manifest_version": 3,
  "name": "Study Kitty",
  "version": "1.0.0",
  "description": "",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "host_permissions": [
    "https://www.youtube.com/*",
    "https://www.facebook.com/*",
    "https://www.netflix.com/*",
    "https://www.roblox.com/*"
  ],
  "permissions": [
    "storage",
    "alarms",
    "notifications",
    "windows"
  ],
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_title": "Timer Extension Action Title",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "Content.js"
      ]
    }
  ],
  "options_page": "options/options.html",
  "web_accessible_resources": [
    {
      "resources": ["icon.png", "AngryKitty.jpg", "file.png"],
      "matches": ["<all_urls>"]
    }
  ]
}